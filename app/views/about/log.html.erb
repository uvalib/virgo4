<%# app/views/about/log.html.erb -%>
<%-
  # Administrator-only log file viewer.
  return unless authorized?

  title ||=
    t('blacklight.about.log.title', env: Rails.env, default: 'Log file')
  page_title ||= title
  lines ||= @lines || []

-%>
<%= render 'about/common', page_title: page_title %>

<%= render 'about/log/viewer', lines: lines %>
